{
  "paths": ["/usr/yoda/lib/component"],
  "stages": [
    {
      "name": "prepare",
      "comps": ["turen", "lifetime", "light", "flora",
        "dbus-registry", "wormhole"]
    },
    {
      "name": "connect",
      "comps": ["dnd-mode"]
    },
    {
      "name": "login",
      "comps": ["keyboard"]
    },
    {
      "name": "ready",
      "comps": ["sound"]
    }
  ],
  "interception": {
    "runtimeDidLogin": [ "ota.runtimeDidLogin" ],
    "turenDidWakeUp": [
      "battery.delegateWakeUpIfDangerousStatus",
      "custodian.turenDidWakeUp",
      "battery.delegateWakeUpIfBatteryInsufficient",
      "ota.turenDidWakeUp"
    ]
  }
}
